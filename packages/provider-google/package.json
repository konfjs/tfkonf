{
  "name": "@konfjs/provider-google",
  "version": "0.1.0",
  "author": {
    "name": "konfjs",
    "url": "https://github.com/konfjs"
  },
  "license": "MPL-2.0",
  "homepage": "https://github.com/konfjs/tfkonf",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/konfjs/tfkonf.git",
    "directory": "packages/provider-google"
  },
  "bugs": {
    "url": "https://github.com/konfjs/tfkonf/issues"
  },
  "keywords": [
    "terraform",
    "cdktf",
    "cdk"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "updatePackage": "yq -i '.exports[\".\"] |= {\"import\": \"./dist/index.js\", \"types\": \"./dist/index.d.ts\" }' package.json",
    "build": "rm -rf dist && tsc",
    "prepack": "bun run build && bun run updatePackage"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@konfjs/core": "workspace:*"
  }
}
