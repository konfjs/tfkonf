{
  "name": "@tfs/provider-generator",
  "version": "0.0.1",
  "author": {
    "name": "shinebayar-g",
    "url": "https://github.com/shinebayar-g"
  },
  "license": "MPL-2.0",
  "homepage": "https://github.com/shinebayar-g/tfs",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/shinebayar-g/tfs.git",
    "directory": "packages/provider-generator"
  },
  "bugs": {
    "url": "https://github.com/shinebayar-g/tfs/issues"
  },
  "keywords": [
    "terraform",
    "cdktf",
    "cdk"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "updatePackage": "yq -i '.exports[\".\"] |= {\"import\": \"./dist/index.js\", \"types\": \"./dist/index.d.ts\" }' package.json",
    "build": "rm -rf dist && tsc",
    "prepack": "bun run build && bun run updatePackage",
    "pull": "terraform providers schema -json > schema.json",
    "gen": "bun run src/index.ts",
    "generate": "bun run gen"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/generator": "7.26.2",
    "@babel/types": "7.26.0",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "tfs": "workspace:*"
  },
  "devDependencies": {
    "@types/babel__generator": "7.6.8"
  }
}
